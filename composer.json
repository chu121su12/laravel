{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "repositories": [
        { "type": "git", "url": "https://gitlab.com/chu121su12/cr-library.git" },
        { "type": "git", "url": "https://gitlab.com/chu121su12/laravel-prev.git" }
    ],
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "^6.5|^7.2",
        "laravel/framework": "dev-merge",
        "tightenco/ziggy": "^0.9"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php composer.phar selfupdate --2.2",
            "@php php-cs-fixer.phar selfupdate"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "stan": "@php phpstan.phar analyze --memory-limit 1G",
        "cs": "@php php-cs-fixer.phar fix --allow-risky no",
        "csr": "@php php-cs-fixer.phar fix --allow-risky yes",
        "lint": "@php lint.php"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "platform": {
            "php": "5.6.5"
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
